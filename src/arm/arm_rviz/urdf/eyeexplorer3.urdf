<?xml version="1.0" ?>
<robot name="eyeexplorer3">
  <!-- Import your models -->
  <material name="ARM/black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="ARM/blue">
    <color rgba="0.0 0.0 0.8 1.0"/>
  </material>
  <material name="ARM/green">
    <color rgba="0.0 0.8 0.0 1.0"/>
  </material>
  <material name="ARM/grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="ARM/orange">
    <color rgba="1.0 0.423529411765 0.0392156862745 1.0"/>
  </material>
  <material name="ARM/brown">
    <color rgba="0.870588235294 0.811764705882 0.764705882353 1.0"/>
  </material>
  <material name="ARM/red">
    <color rgba="0.8 0.0 0.0 1.0"/>
  </material>
  <material name="ARM/white">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>
  
  <!-- Build your comprehensive robot -->
  <link name="world"/>

  <joint name="frame_joint" type="fixed">
    <parent link="world"/>
    <child link="alm_frame"/>
  </joint>

  <link name="alm_frame">
    <visual>
      <geometry>
        <box size = "0.5 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.25 0 0"/>
      <material name="ARM/grey"/>
    </visual>
  </link>

  <joint name="base_plate" type="fixed">
    <parent link="alm_frame"/>
    <child link="base"/>
    <origin rpy="0 0 0" xyz="0.5 0 0"/>
  </joint>

  <link name="base">
    <visual>
      <geometry>
        <box size = "0.03 0.03 0.08"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.04"/>
      <material name="ARM/grey"/>
    </visual>
  </link>

  <joint name="arm_joint1" type="continuous">
    <parent link="base"/>
    <child link="arm_link1"/>
    <origin rpy="0 0 0" xyz="0 0 0.08"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="arm_link1">
    <visual>
      <geometry>
        <box size = "0.03 0.03 0.032"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.009 0 0.016"/>
      <material name="ARM/orange"/>
    </visual>
  </link>

  <joint name="arm_joint2" type="continuous">
    <parent link="arm_link1"/>
    <child link="arm_link2"/>
    <origin rpy="0 0 0" xyz="0.009 0 0.032"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="arm_link2">
    <visual>
      <geometry>
        <box size="0.2 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.1 0 0"/>
      <material name="ARM/orange"/>
    </visual>
  </link>

  <joint name="arm_joint3" type="continuous">
    <parent link="arm_link2"/>
    <child link="arm_link3"/>
    <origin rpy="0 0 0" xyz="0.2 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="arm_link3">
    <visual>
      <geometry>
        <box size="0.03 0.03 -0.2"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.03232 0 -0.08232"/>
      <material name="ARM/orange"/>
    </visual>
  </link>

  <joint name="arm_joint_horiz" type="continuous">
    <parent link="arm_link3"/>
    <child link="arm_link4"/>
    <origin rpy="0 0 0" xyz="0.03232 0 -0.18232"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="arm_link4">
    <visual>
      <geometry>
        <box size="0.0349 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.01745 0 0"/>
      <material name="ARM/orange"/>
    </visual>
  </link>

  <joint name="arm_joint4" type="continuous">
    <parent link="arm_link4"/>
    <child link="arm_link50"/>
    <origin rpy="0 0 0" xyz="0.0349 0 0.0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <link name="arm_link50">
    <visual>
      <geometry>
        <box size="0.02 -0.06 0.02"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 -0.03 0"/>
      <material name="ARM/red"/>
    </visual>
  </link>
  
  <joint name="arm_joint_gimbal" type="fixed">
    <parent link="arm_link50"/>
    <child link="arm_link51"/>
    <origin rpy="0 0 0" xyz="0 -0.06 0"/>
  </joint>

  <link name="arm_link51">
    <visual>
      <geometry>
        <box size="0.06 0.02 0.02"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.03 0 0"/>
      <material name="ARM/red"/>
    </visual>
  </link>

  <joint name="arm_joint5" type="continuous">
    <parent link="arm_link51"/>
    <child link="arm_link6"/>
    <origin rpy="0 0 0" xyz="0.06 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="arm_link6">
    <visual>
      <geometry>
        <box size="0.02 0.06 0.02"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0.03 0"/>
      <material name="ARM/red"/>
    </visual>
  </link>

  <joint name="endoscope_joint" type="fixed">
    <parent link="arm_link6"/>
    <child link="endoscope_link"/>
    <origin rpy="0 0 0" xyz="0 0.06 0"/>
  </joint>

  <link name="endoscope_link">
    <visual>
      <geometry>
        <box size="0.01 0.01 -0.045"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 -0.0225"/>
      <material name="ARM/black"/>
    </visual>
  </link>

  <joint name="endoscope_joint2" type="fixed">
    <parent link="endoscope_link"/>
    <child link="endoscope_link2"/>
    <origin rpy="0 0 0" xyz="0 0 -0.0225"/>
  </joint>

  <link name="endoscope_link2">
    <visual>
      <geometry>
        <box size="0.005 0.005 -0.045"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 -0.0225"/>
      <material name="ARM/black"/>
    </visual>
  </link>

  <joint name="endoscope_camera" type="fixed">
    <parent link="endoscope_link2"/>
    <child link="endoscope"/>
    <origin rpy="0 3.14 -3.14" xyz="0 0 0"/>
  </joint>

  <link name="endoscope">
    <visual>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="ARM/black"/>
    </visual>
  </link>

</robot>

